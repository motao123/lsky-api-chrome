{
  "manifest_version": 3,
  "name": "Lsky Pro 图片上传助手",
  "version": "1.1.0",
  "description": "优雅的兰空图床上传插件，支持多种格式导出",
  "author": "Lsky Pro",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "options_page": "options.html",
  "action": {
    "default_title": "Lsky Pro 图片上传助手",
    "default_popup": "popup.html"
  }
}